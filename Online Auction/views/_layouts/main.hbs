								<a href="index.html">Online<span>Auction</span></a>
							</div>
							<nav class="navbar">
								<ul class="navbar_menu">
									<li><a href="index.html">home</a></li>
									<li>
										<a href="categories.html" class="dropdown-toggle"
											data-toggle="dropdown">Categories<b class="caret"></b></a>
										<ul class="dropdown-menu">
											<li class="dropdown-submenu">
												{{#each lcCatLevel1}}
												<a href="/user/categories/{{CatID}}/products"
													class="dropdown-toggle">{{CatName}}</a>
												<ul class="dropdown-menu">
													{{#each ../lcCatAll}}
													{{#if_eq ParentID ../CatID}}
													<a href="/user/categories/{{CatID}}/products">{{CatName}}</a>
													{{/if_eq}}
													{{/each}}
												</ul>
												{{/each}}
											</li>
										</ul>
									</li>
									<li><a href="#">your auction</a></li>
									<li>
										<form class="form-inline my-2 my-lg-0 mr-2">
											<input class="form-control mr-sm-2" type="search" placeholder="Search"
												aria-label="Search">
											<button class="btn btn-outline-success my-2 my-sm-0"
												type="submit">Search</button>
										</form>
									</li>
								</ul>
								<ul class="navbar_user">
									<li>
										<a href="bidder_profile.html"><i class="fa fa-2x fa-user"
												aria-hidden="true"></i></a>
									</li>
									<li class="checkout ml-3">
										<a href="#">
											<i class="fa fa-heart" aria-hidden="true"></i>
											<span id="checkout_items" class="checkout_items">2</span>
										</a>
									</li>
								</ul>
								<div class="hamburger_container">
									<i class="fa fa-bars" aria-hidden="true"></i>
