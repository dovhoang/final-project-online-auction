											style="cursor: pointer;">Categories<b></b></a>
										<ul class="dropdown-menu ">
											{{#each lcCatLevel1}}
											<li class="dropdown-submenu">
												<a href="/user/categories/{{CatID}}/products"
													class="dropdown-toggle">{{CatName}}</a>
												<ul class="dropdown-menu">
													{{#each ../lcCatAll}}
													{{#if_eq ParentID ../CatID}}
													<li><a href="/user/categories/{{CatID}}/products">{{CatName}}</a>
													</li>
													{{/if_eq}}
													{{/each}}
												</ul>
											</li>
											{{/each}}
										</ul>
									</li>
									<li><a href="#">your auction</a></li>
									<li>
										<form class="form-inline my-2 my-lg-0 mr-2">
